{
  "name": "api-service",
  "version": "1.0.0",
  "description": "Family Tree API Service - Handles logic and biographical data",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "prisma generate && tsc",
